SHELL           = @SHELL@

_I              = @INCLUDEFLAG@../mpeu @INCLUDEFLAG@../mct @INCLUDEPATH@
_L              = -L../mpeu -L../mct
_l              = -lmct -lmpeu @MPILIBS@

LD              = $(FC)
LDFLAGS         = $(FCFLAGS)

DEFS            = @MACHDEFS@

FPP		= @FPP@
FPPFLAGS	= @FPPFLAGS@

CC		= @CC@

FC		= @FC@
FCFLAGS         = @FCFLAGS@ @BIT64@ @REAL8@ @ENDIAN@
F90FLAGS	= @F90FLAGS@


@FRULE@:
	$(FC) -c $(_I) $(DEFS) $(FCFLAGS) $*.F

@F90RULE@:
	$(FC) -c $(_I) $(DEFS) $(FCFLAGS) $(F90FLAGS) $*.F90

@FRULECPP@:
	$(FPP) $(FPPFLAGS) $(DEFS) $*.F $*.f
	$(FC) -c $(FCFLAGS) $(_I) $*.f
	$(RM) $*.f

@F90RULECPP@:
	$(FPP) $(FPPFLAGS) $(DEFS) $*.F90 $*.f90
	$(FC) -c $(FCFLAGS) $(F90FLAGS) $(_I) $*.f90
	$(RM) $*.f90

.SUFFIXES: .H .h .F90 .f90 .F .f .c .o

#.----------------------------------------------------------------------

